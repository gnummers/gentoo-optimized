(?su)[\\]?\+\+\+(?P<passtext>.*?)\+\+\+=pass[]
(?su)[\\]?\$\$(?P<passtext>.*?)\$\$=pass[specialcharacters]
(?su)(?<!\w)([\\]?`(?P<passtext>\S|\S.*?\S)`)(?![`\w])=literal[specialcharacters]
