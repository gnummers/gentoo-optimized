source /etc/network/interfaces.d/*

auto lo
iface lo inet loopback
iface lo inet6 loopback

auto 
iface  inet static
  address 95.217.117.234
  netmask 255.255.255.192
  gateway 95.217.117.193
  # route 95.217.117.192/26 via 95.217.117.193
  up route add -net 95.217.117.192 netmask 255.255.255.192 gw 95.217.117.193 dev 

iface  inet6 static
  address 2a01:4f9:4a:3355::2
  netmask 64
  gateway fe80::1
