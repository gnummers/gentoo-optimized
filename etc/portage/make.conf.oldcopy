MAKEOPTS="-j13"
ACCEPT_LICENSE="*"
ACCEPT_KEYWORDS="~amd64"
#ALSA_CARDS="hda-intel"
CPU_FLAGS_X86="aes avx avx2 f16c fma3 mmx mmxext pclmul popcnt sha sse sse2 sse3 sse4_1 sse4_2 sse4a ssse3"
_NO_SSP="-fno-stack-protector -fno-stack-clash-protection -fstack-check=no -fno-stack-check"
CFLAGS="-march=native ${CFLAGS} -pipe -g0 ${_NO_SSP}" #-msse4.2
CHOST="x86_64-pc-linux-gnu"
CXXFLAGS="${CFLAGS}"
DISTDIR="${PORTDIR}/distfiles"
EGIT_STORE_DIR="/tmp"
#EMERGE_DEFAULT_OPTS="--with-bdeps y --keep-going y --quiet-fail n --jobs 3"
#EMERGE_DEFAULT_OPTS="--with-bdeps y --complete-graph y --keep-going y --quiet-fail n --jobs 4 --load-average 6.0"
EMERGE_DEFAULT_OPTS="--with-bdeps y --complete-graph y --keep-going n --quiet-fail n"
ESVN_STORE_DIR="/tmp"
FEATURES="-collision-protect -protect-owned multilib-strict userpriv usersandbox usersync userfetch binpkg-logs binpkg-multi-instance buildpkg candy cgroup compress-index downgrade-backup fixlafiles news nodoc parallel-fetch parallel-install -ebuild-locks unmerge-backup unmerge-logs unmerge-orphans xattr"
GENTOO_MIRRORS="http://ftp.fau.de/gentoo"
GRUB_PLATFORMS="pc efi-64"
INPUT_DEVICES="keyboard mouse evdev synaptics"
L10N="en en_US en-US"
LC_MESSAGES=C
LDFLAGS="${LDFLAGS} -Wl,--hash-style=gnu -Wl,-z,norelro"
LINGUAS="en en_US"
NGINX_MODULES_HTTP="access fastcgi gzip proxy rewrite"
PKGDIR="${PORTDIR}/packages"
PORTAGE_BINHOST="http://packages.gentooexperimental.org/packages/amd64-unstable/"
PORTAGE_ELOG_CLASSES="qa warn"
#PORTAGE_NICENESS="19"
PORTAGE_NICENESS="-19"
PORTAGE_TMPDIR="/tmp"
PORTDIR_OVERLAY="/usr/local/portage"
PORTDIR="/usr/portage"
PORT_LOGDIR="/var/log/emerge-log"
PYTHON_SINGLE_TARGET="python3_8"
PYTHON_TARGETS="python3_8 python3_7 python3_6 python2_7"
QEMU_SOFTMMU_TARGETS="x86_64"
RUBY_TARGETS="ruby27 ruby26 ruby25"
VIDEO_CARDS="nvidia nouveau"
LLVM_TARGETS="AMDGPU BPF X86 AArch64 ARM NVPTX"
QEMU_SOFTMMU_TARGETS="x86_64 aarch64 arm"
QEMU_USER_TARGETS="x86_64 aarch64 arm"
